<div class="livescore-tabs">
	<div class="tab">
	    <a href="<%= constant.SITE_URL %>firstcricket/series/<%= seriesSlug %>.html" title="<%= seriesName %>" class="tablinks <% if(menuHighlight == 'seriesHome'){%>active<%}%>"><%= seriesName %></a>

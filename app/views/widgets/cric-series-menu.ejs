	    	<a href="<%= constant.SITE_URL %>firstcricket/points-table/series/<%= seriesSlug %>.html" title="<%= seriesName %> Points Table"  class="tablinks <% if(menuHighlight == 'points-table'){%>active<%}%>">Points Table</a>
	    <%}%>
		<a href="<%= constant.SITE_URL %>firstcricket/sports-news/series/<%= seriesSlug %>" title="<%= seriesName %> News"  class="tablinks <% if(menuHighlight == 'seriesNews'){%>active<%}%>">News</a>		

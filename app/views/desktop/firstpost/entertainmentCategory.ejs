		            shareArr['shareTitle'] = catData_val['title'];
                  	shareArr['shareUrl'] = catData_val['url']; 
		       %>
		       <%- include(viewpath+ '/widgets/popup_social_share_widget',{shareArr:shareArr}); %>
            </div>
         </div>
        <% } %>
      <% } %> 


<%- include(viewpath+ '/layouts/header'); %>

<div class="container">
   <% if (typeof ads  !== 'undefined' && typeof ads['ATF_728']  !== 'undefined'){%>
        <div class="ad-center fixed-ad-banner">
            <%-common.displayAds(ads['ATF_728'][0]) %>
        </div>
    <% } %> 
   <!--cricket-ticker-wrap-->
   <%- include(viewpath+ '/widgets/cricketscore_strip'); %>
   <!--cricket-ticker-wrap-->
   
   <%- include(viewpath+ '/widgets/breadCrumbs'); %>
   <div class="main-container">
      <div class="main-content">
      	<% if((pgNo == '1' || typeof pgNo =='undefined') && categoryData !== null) { %> 
         <div class="main-story-thumb-wrap">
            <h3 class="main-story-title">
               <a href="javascript:void(0);"><%=category_name %></a>
               <span></span>
            </h3>
            <div class="slider-wrap main-story-slider" id="mainstoryslider">
               <div class="glide__track" data-glide-el="track">
                  <ul class="slider">
                  	 <% for (const [categoryData_key, categoryData_val] of Object.entries(categoryData['CatMainRanking']['main_ranking'])) { %>
                     <li class="main-story-thumb">
                        <% if(categoryData_val['isliveBlog'] != 0) { %> 
		                  <div class="live-txt"><span class="blinking"></span>live</div>
		                <% } %>
                        <a href="<%- categoryData_val['modified_posturl'].replace(constant.MAIN_SITE_URL, constant.SITE_URL)%>" class="thumb">
                        <img src="<%- categoryData_val['desktop_img']%>" alt="<%- categoryData_val['modified_title']%>" title="<%- categoryData_val['modified_title']%>">
                        </a>
                        <div class="title-wrap">
                        	<a href="<%-categoryData_val['category_url'] %>" class="category-name"><%-categoryData_val['category'] %></a>
                           <h1 class="main-title">
                              <a href="<%-categoryData_val['modified_posturl'].replace(constant.MAIN_SITE_URL, constant.SITE_URL) %>"><%-categoryData_val['modified_title'] %></a>
                           </h1>
                           <% var shareArr = []; 
		                        shareArr['shareTitle'] = categoryData_val['modified_title'];
		                        shareArr['shareUrl'] = categoryData_val['modified_posturl']; 
		                   %>
		                   <%- include(viewpath+ '/widgets/popup_social_share_widget',{shareArr:shareArr}); %>
                        </div>
                     </li>
                    <% } %>
                  </ul>
                  <div class="glide__bullets" data-glide-el="controls[nav]">
			<% i = 0; %>
                  	<% for (const [categoryData_key, categoryData_val] of Object.entries(categoryData['CatMainRanking']['main_ranking'])) { %>
		               <button class="glide__bullet" data-glide-dir="=<%=i %>"></button>
			   <% i++; %>
			<% } %>
                  </div>
               </div>
            </div>
         </div>
         <!--main-story-thumb-->
         <% for (const [categoryData_key, categoryData_val] of Object.entries(categoryData['CatMainRanking']['related_ranking'])) { %>
         <div class="big-thumb">
            <a href="<%- categoryData_val['modified_posturl'].replace(constant.MAIN_SITE_URL, constant.SITE_URL)%>" class="thumb-img">
            	<img class="fp-lazy" src="<%-constant.PLACEHOLDER_IMG_640x362 %>" data-src="<%-common.firstpost_imageOptimization(categoryData_val['post_image'], 640, 362, '') %>" alt="<%- categoryData_val['modified_title']%>" title="<%- categoryData_val['modified_title']%>">
            </a>
            <div class="title-wrap">
               <a href="<%- categoryData_val['category_url']%>" class="category-name"><%- categoryData_val['category']%>
               </a>
               <h3 class="main-title">
                  <a href="<%- categoryData_val['modified_posturl'].replace(constant.MAIN_SITE_URL, constant.SITE_URL)%>"><%= categoryData_val['modified_title']%></a>
               </h3>
               <p class="copy">
                  <%= categoryData_val['post_excerpt']%>
               </p>
               <% var shareArr = []; 
		            shareArr['shareTitle'] = categoryData_val['modified_title'];
		            shareArr['shareUrl'] = categoryData_val['modified_posturl']; 
		       %>
		       <%- include(viewpath+ '/widgets/popup_social_share_widget',{shareArr:shareArr}); %>
            </div>
         </div>
        <% } %>
         <!--big-thumb-->

         <% if (typeof ads  !== 'undefined' && (typeof ads['MID1_300']  !== 'undefined' || typeof ads['MID2_300']  !== 'undefined')){%>
              <div class="ad-combo">
              <% if (typeof ads  !== 'undefined' && typeof ads['MID1_300']  !== 'undefined'){%>
                  <div>
                      <%-common.displayAds(ads['MID1_300'][0]) %>
                  </div>
              <% } %> 
              <% if (typeof ads  !== 'undefined' && typeof ads['MID2_300']  !== 'undefined'){%>
                  <div>
                      <%-common.displayAds(ads['MID2_300'][0]) %>
                  </div>
              <% } %>  
              </div>
         <% } %>
         
         <!-- Video section-->
         <div class="small-thumb-wrap">
            <h3 class="main-story-title">
               <a href="javascript:void(0);">videos</a>
               <span></span>
            </h3>
            <div class="small-thumb-row">
            <% for (const [videoDataArr_key, videoDataArr_val] of Object.entries(videoDataArr['solarResults'].slice(0,3))) { %>
               <div class="small-thumb">
                  <a href="<%- videoDataArr_val['url']%>" class="thumbnail">
                     <img class="fp-lazy" src="<%-constant.PLACEHOLDER_IMG_192x104 %>" data-src="<%-common.firstpost_imageOptimization(videoDataArr_val['image'], 192, 104, '') %>" alt="<%- videoDataArr_val['title']%>" title="<%- videoDataArr_val['title']%>">
                     <svg width="70.182" height="70.182" viewBox="0 0 70.182 70.182" class="video-icon">
                        <defs>
                           <filter id="a" x="0" y="0" width="70.182" height="70.182" filterUnits="userSpaceOnUse">
                              <feOffset dy="3" input="SourceAlpha"></feOffset>
                              <feGaussianBlur stdDeviation="4.5" result="b"></feGaussianBlur>
                              <feFlood flood-opacity="0.294"></feFlood>
                              <feComposite operator="in" in2="b"></feComposite>
                              <feComposite in="SourceGraphic"></feComposite>
                           </filter>
                        </defs>
                        <g transform="matrix(1, 0, 0, 1, 0, 0)">
                           <path fill="#fff" d="M23.591,2A21.591,21.591,0,1,0,45.182,23.591,21.6,21.6,0,0,0,23.591,2ZM19.273,33.307V13.875l12.955,9.716Z" transform="translate(11.5 8.5)"></path>
                        </g>
                     </svg>
                  </a>
                  <a href="<%- videoDataArr_val['url']%>" class="copy">
                     <%-videoDataArr_val['title'] %>
                  </a>
                  <!--<a href="" class="duration">
                  09:25
                  </a>-->
                  <% var shareArr = []; 
                     shareArr['shareTitle'] = videoDataArr_val['title'];
                     shareArr['shareUrl'] = videoDataArr_val['url']; 
                  %>
                  <%- include(viewpath+ '/widgets/popup_social_share_widget',{shareArr:shareArr}); %>
               </div>
            <% } %>
            </div>
         </div>
         <!--trending-->

       	<% for (const [categoryData_key, categoryData_val] of Object.entries(categoryData['CatLatestStories'])) { %>
         <div class="big-thumb">
            <a href="<%- categoryData_val['url'].replace(constant.MAIN_SITE_URL, constant.SITE_URL)%>" class="thumb-img">
            	<img class="fp-lazy" src="<%-constant.PLACEHOLDER_IMG_640x362 %>" data-src="<%-common.firstpost_imageOptimization(categoryData_val['post_image'], 640, 362, '') %>" alt="<%- categoryData_val['title']%>" title="<%- categoryData_val['title']%>">
            </a>
            <div class="title-wrap">
               <a href="<%- categoryData_val['category_url']%>" class="category-name"><%- categoryData_val['category']%>
               </a>
               <h3 class="main-title">
                  <a href="<%- categoryData_val['url'].replace(constant.MAIN_SITE_URL, constant.SITE_URL)%>"><%= categoryData_val['title']%></a>
               </h3>
               <p class="copy">
                 	<%= categoryData_val['post_excerpt']%>
               </p>
               <% var shareArr = []; 
		            shareArr['shareTitle'] = categoryData_val['title'];
            		shareArr['shareUrl'] = categoryData_val['url'];  
		       %>
		       <%- include(viewpath+ '/widgets/popup_social_share_widget',{shareArr:shareArr}); %>
            </div>
         </div>
        <% } %>
        <!--big-thumb-->
     <% } else { %>   
     <% for (const [catData_key, catData_val] of Object.entries(catData['solarResults'])) { %>
         <div class="big-thumb">
            <a href="<%- catData_val['url'].replace(constant.MAIN_SITE_URL, constant.SITE_URL)%>" class="thumb-img">

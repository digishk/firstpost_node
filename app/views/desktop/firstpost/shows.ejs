<%- include(viewpath+ '/layouts/header'); %>
	<div class="container">
		<% if (typeof ads  !== 'undefined' && typeof ads['ATF_728']  !== 'undefined'){%>
	        <div class="ad-center">
	            <%-common.displayAds(ads['ATF_728'][0]) %>
	        </div>
    	<% } %>
	    <!--cricket-ticker-wrap-->
    <%- include(viewpath+ '/widgets/breadCrumbs'); %>

    <div class="cricket-ticker-wrap">
		<div class="slider-wrap cricket-ticker-slider showtickerslider" id="showtickerslider">
			<div class="glide__track" data-glide-el="track">
				<div class="slider">
					<% for (const [showList_key, showList_val] of Object.entries(showListing)) { %>
					<a href="/shows/<%-showList_val['slug'].replace(constant.MAIN_SITE_URL, constant.SITE_URL) %>" class=""><img src="<%-common.firstpost_imageOptimization(showList_val['img_logo_url']) %>"></a>
					<% } %>
					
				</div>
			</div>
			<div class="glide__arrows" data-glide-el="controls">
                <div class="glide__arrow glide__arrow--prev" data-glide-dir="<"><svg height="24" viewBox="0 0 24 24" width="24"><path d="M15.41 16.59L10.83 12l4.58-4.59L14 6l-6 6 6 6 1.41-1.41z"/><path d="M0 0h24v24H0V0z" fill="none"/></svg></div>
                <div class="glide__arrow glide__arrow--next" data-glide-dir=">"><svg height="24" viewBox="0 0 24 24" width="24"><path d="M8.59 16.59L13.17 12 8.59 7.41 10 6l6 6-6 6-1.41-1.41z"/><path d="M0 0h24v24H0V0z" fill="none"/></svg></div>
            </div>
		</div>
	</div><!--cricket-ticker-wrap-->


	        <div class="main-container" id="showsLanding">
	        	<div class="main-content">
	        		<div class="main-story-thumb-wrap">
						<h3 class="main-story-title">
							<a href="">SHOWS</a>
							<span></span>
						</h3>
						<div class="slider-wrap main-story-slider" id="mainstoryslider">
				            <div class="glide__track" data-glide-el="track">
				                <ul class="slider">
				                	<% for (const [showList_key, showList_val] of Object.entries(showListing)) { %>
									<li class="main-story-thumb">
										<!-- <div class="live-txt"><span class="blinking"></span>live</div> -->
									   <a href="<%-showList_val[0]['PostURL'].replace(constant.MAIN_SITE_URL, constant.SITE_URL) %>" class="thumb">
									   		<img class="fp-lazy" src="<%-constant.PLACEHOLDER_IMG_640x362 %>" data-src="<%-common.firstpost_imageOptimization(showList_val[0]['FeaturedImage'], 640, 362, '') %>" alt="<%-showList_val[0]['PostTitle'] %>" title="<%-showList_val[0]['PostTitle'] %>">
									   		<div class="video-icon">
									   			<svg width="70.182" height="70.182" viewBox="0 0 70.182 70.182"><defs><filter id="a" x="0" y="0" width="70.182" height="70.182" filterUnits="userSpaceOnUse"><feOffset dy="3" input="SourceAlpha"></feOffset><feGaussianBlur stdDeviation="4.5" result="b"></feGaussianBlur><feFlood flood-opacity="0.294"></feFlood><feComposite operator="in" in2="b"></feComposite><feComposite in="SourceGraphic"></feComposite></filter></defs><g transform="matrix(1, 0, 0, 1, 0, 0)"><path fill="#fff" d="M23.591,2A21.591,21.591,0,1,0,45.182,23.591,21.6,21.6,0,0,0,23.591,2ZM19.273,33.307V13.875l12.955,9.716Z" transform="translate(11.5 8.5)"></path></g></svg>
									   		</div>
									   </a>
									   <div class="title-wrap">
									   		<a href="javascript:void(0);" class="category-name">Shows</a>
									   		<h1 class="main-title">
									   			<a href="<%-showList_val[0]['PostURL'].replace(constant.MAIN_SITE_URL, constant.SITE_URL) %>"><%-showList_val[0]['PostTitle'] %></a>
									   		</h1>
									   		<% var shareArr = []; 
								            shareArr['shareTitle'] = showList_val[0]['PostTitle'];
								            shareArr['shareUrl'] = showList_val[0]['PostURL'].replace(constant.MAIN_SITE_URL, constant.SITE_URL); 
									       %>
									       <%- include(viewpath+ '/widgets/popup_social_share_widget',{shareArr:shareArr}); %>	
									   </div>
									</li>
									<% } %>
								</ul> 
						        <div class="glide__bullets" data-glide-el="controls[nav]">
						        <% var i=0; for (const [showList_key, showList_val] of Object.entries(showListing)) { %>	
				                    <button class="glide__bullet" data-glide-dir="=<%=i %>"></button>
				                <% i++; } %>        
				                </div>   
				          	</div>
				    	</div>
					</div><!--main-story-thumb-->
					
					<%
					var show_cnt = 1;
					for (const [showList_key, showList_val] of Object.entries(showListing)) { %>	
					<div class="photos-wrap">
						<h3 class="main-story-title">
							<a href=""><%-showList_key %></a>
							<span></span>
							<a href="<%-constant.SITE_URL+'shows/'+showList_val['slug'] %>" class="h-sponsor-logo"><img src="<%-common.firstpost_imageOptimization(showList_val['img_logo_url'], 60, 60, '') %>"></a>
						</h3>

					</div>

					<div class="videos small-thumb-wrap">
						<div class="small-thumb-row">
							<% for (const [show_key, show_val] of Object.entries(showList_val)) { %>
							<% if (show_key > 0 && show_key < 4) { %>	
							<div class="small-thumb">
								<a href="<%-show_val['PostURL'].replace(constant.MAIN_SITE_URL, constant.SITE_URL) %>" class="thumbnail">
									<img class="fp-lazy" src="<%-constant.PLACEHOLDER_IMG_192x104 %>" data-src="<%-common.firstpost_imageOptimization(show_val['PostImageThumbLarge'], 192, 104, '') %>" alt="<%-show_val['PostTitle'] %>" title="<%-show_val['PostTitle'] %>">
									<svg width="70.182" height="70.182" viewBox="0 0 70.182 70.182" class="video-icon"><defs><filter id="a" x="0" y="0" width="70.182" height="70.182" filterUnits="userSpaceOnUse"><feOffset dy="3" input="SourceAlpha"></feOffset><feGaussianBlur stdDeviation="4.5" result="b"></feGaussianBlur><feFlood flood-opacity="0.294"></feFlood><feComposite operator="in" in2="b"></feComposite><feComposite in="SourceGraphic"></feComposite></filter></defs><g transform="matrix(1, 0, 0, 1, 0, 0)"><path fill="#fff" d="M23.591,2A21.591,21.591,0,1,0,45.182,23.591,21.6,21.6,0,0,0,23.591,2ZM19.273,33.307V13.875l12.955,9.716Z" transform="translate(11.5 8.5)"></path></g></svg>
								</a>
								<a href="<%-show_val['PostURL'].replace(constant.MAIN_SITE_URL, constant.SITE_URL) %>" class="copy">
									<%-show_val['PostTitle'] %>
								</a>
								<div class="trending-bottom">
									<div class="duration"><%-show_val['VideoViews'] %></div>
								<% var shareArr = []; 
						            shareArr['shareTitle'] = show_val['PostTitle'].replace(constant.MAIN_SITE_URL, constant.SITE_URL);
						            shareArr['shareUrl'] = show_val['PostURL']; 
							    %>
						        <%- include(viewpath+ '/widgets/popup_social_share_widget',{shareArr:shareArr}); %>	
								</div>
							</div>
							<% } %>
							<% } %>
						</div>

				<a href="<%-constant.SITE_URL+'shows/'+showList_val['slug'] %>" class="see-all">View All <svg viewBox="0 0 24 24"><path d="M8.59 16.59L13.17 12 8.59 7.41 10 6l6 6-6 6-1.41-1.41z"></path><path d="M0 0h24v24H0V0z" fill="none"></path></svg></a>
					</div><!--videos-small-thumb-wrap-->
					<% if(show_cnt == 5 && typeof outbrain_ads  !== 'undefined' && typeof outbrain_ads['AR_2']  !== 'undefined'){%>
						<%-outbrain_ads['AR_2'];%>		
					<% }
						show_cnt++
					} %>
					
	        	</div><!--main-content-->
	        	<%- include(viewpath+ '/'+device+'/firstpost/rhs'); %>
	        </div>	        
		</div><!--container-->

<%- include(viewpath+ '/layouts/footer'); %>
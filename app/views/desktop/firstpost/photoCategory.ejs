               </div>
            </a>
            <div class="title-wrap">
               <a href="<%- categoryData_val['category_url']%>" class="category-name"><%- categoryData_val['category']%>
               </a>
               <h3 class="main-title">
                  <a href="<%- categoryData_val['modified_posturl'].replace(constant.MAIN_SITE_URL, constant.SITE_URL)%>"><%= categoryData_val['modified_title']%></a>
               </h3>
               <p class="copy">
                  <%= categoryData_val['post_excerpt']%>
               </p>
               <% var shareArr = []; 
		            shareArr['shareTitle'] = categoryData_val['modified_title'];
		            shareArr['shareUrl'] = categoryData_val['modified_posturl']; 
		       %>
		       <%- include(viewpath+ '/widgets/popup_social_share_widget',{shareArr:shareArr}); %>
            </div>
         </div>
         <% } %>
         <!--big-thumb-->
         <% for (const [categoryData_key, categoryData_val] of Object.entries(categoryData['CatLatestStories'])) { %>
         <div class="big-thumb">
            <a href="<%- categoryData_val['url']%>" class="thumb-img">
            	<img class="fp-lazy" src="<%-constant.PLACEHOLDER_IMG_640x362 %>" data-src="<%-common.firstpost_imageOptimization(categoryData_val['post_image'], 640, 362, '') %>" alt="<%- categoryData_val['title']%>" title="<%- categoryData_val['title']%>">
            	<div class="photo-icon">
                  <svg width="50.45" height="45.239" viewBox="0 0 50.45 45.239"><defs><filter id="a" x="0" y="0" width="50.45" height="45.239" filterUnits="userSpaceOnUse"><feOffset dy="3" input="SourceAlpha"></feOffset><feGaussianBlur stdDeviation="4.5" result="b"></feGaussianBlur><feFlood flood-opacity="0.161"></feFlood><feComposite operator="in" in2="b"></feComposite><feComposite in="SourceGraphic"></feComposite></filter></defs><g transform="translate(13.5 10.5)"><g style="filter:url(#a);" transform="matrix(1, 0, 0, 1, -13.5, -10.5)"><path style="fill:#000;" d="M23.147,3H2.3A1.3,1.3,0,0,0,1,4.3V19.936a1.3,1.3,0,0,0,1.3,1.3H23.147a1.3,1.3,0,0,0,1.3-1.3V4.3A1.3,1.3,0,0,0,23.147,3ZM16.959,7.56A1.628,1.628,0,1,1,15.33,9.188,1.628,1.628,0,0,1,16.959,7.56ZM4.908,17.33,9.226,7.4l4.91,7.949,4.209-2.091,2.2,4.067Z" transform="translate(12.5 7.5)"></path></g></g></svg>
               </div>
            </a>
            <div class="title-wrap">
               	<a href="<%- categoryData_val['category_url']%>" class="category-name"><%- categoryData_val['category']%>
               </a>
               <h3 class="main-title">
                  <a href="<%- categoryData_val['url']%>"><%= categoryData_val['title']%></a>
               </h3>
               <p class="copy">
                 	<%= categoryData_val['post_excerpt']%>
               </p>
               <% var shareArr = []; 
		            shareArr['shareTitle'] = categoryData_val['title'];
            		shareArr['shareUrl'] = categoryData_val['url'];  
		       %>
		       <%- include(viewpath+ '/widgets/popup_social_share_widget',{shareArr:shareArr}); %>
            </div>
         </div>
        <% } %>
        <!--big-thumb-->
    <% } else { %>   
     	<% for (const [catData_key, catData_val] of Object.entries(catData['solarResults'])) { %>
         <div class="big-thumb">
            <a href="<%- catData_val['url']%>" class="thumb-img">
            	<img class="fp-lazy" src="<%-constant.PLACEHOLDER_IMG_640x362 %>" data-src="<%-common.firstpost_imageOptimization(catData_val['image'], 640, 362, '') %>" alt="<%- catData_val['title']%>" title="<%- catData_val['title']%>">
            	<div class="photo-icon">
                  <svg width="50.45" height="45.239" viewBox="0 0 50.45 45.239"><defs><filter id="a" x="0" y="0" width="50.45" height="45.239" filterUnits="userSpaceOnUse"><feOffset dy="3" input="SourceAlpha"></feOffset><feGaussianBlur stdDeviation="4.5" result="b"></feGaussianBlur><feFlood flood-opacity="0.161"></feFlood><feComposite operator="in" in2="b"></feComposite><feComposite in="SourceGraphic"></feComposite></filter></defs><g transform="translate(13.5 10.5)"><g style="filter:url(#a);" transform="matrix(1, 0, 0, 1, -13.5, -10.5)"><path style="fill:#000;" d="M23.147,3H2.3A1.3,1.3,0,0,0,1,4.3V19.936a1.3,1.3,0,0,0,1.3,1.3H23.147a1.3,1.3,0,0,0,1.3-1.3V4.3A1.3,1.3,0,0,0,23.147,3ZM16.959,7.56A1.628,1.628,0,1,1,15.33,9.188,1.628,1.628,0,0,1,16.959,7.56ZM4.908,17.33,9.226,7.4l4.91,7.949,4.209-2.091,2.2,4.067Z" transform="translate(12.5 7.5)"></path></g></g></svg>
               </div>
            </a>
            <div class="title-wrap">
               	<a href="<%=constant.SITE_URL%>category/<%- catData_val['category'].replace(/\s+/g, '-').toLowerCase()%>" class="category-name"><%- catData_val['category']%></a>
               </a>
               <h3 class="main-title">
                  <a href="<%- catData_val['url']%>"><%= catData_val['title']%></a>
               </h3>
               <p class="copy">
                 	<%= catData_val['description']%>
               </p>
               <% var shareArr = []; 
		            shareArr['shareTitle'] = catData_val['title'];
                  	shareArr['shareUrl'] = catData_val['url']; 
		       %>
		       <%- include(viewpath+ '/widgets/popup_social_share_widget',{shareArr:shareArr}); %>
            </div>
         </div>
        <% } %>
      <% } %> 
          <%- include(viewpath+ '/widgets/paginationDefault'); %>

         <% if (typeof ads  !== 'undefined' && (typeof ads['MID1_300']  !== 'undefined' || typeof ads['MID2_300']  !== 'undefined')){%>
              <div class="ad-combo">
              <% if (typeof ads  !== 'undefined' && typeof ads['MID1_300']  !== 'undefined'){%>
                  <div>
                      <%-common.displayAds(ads['MID1_300'][0]) %>
                  </div>
              <% } %> 
              <% if (typeof ads  !== 'undefined' && typeof ads['MID2_300']  !== 'undefined'){%>
                  <div>
                      <%-common.displayAds(ads['MID2_300'][0]) %>
                  </div>
              <% } %>  
              </div>
         <% } %>
         <% if( typeof outbrain_ads  !== 'undefined' && typeof outbrain_ads['AR_2']  !== 'undefined'){%>
	    <%-outbrain_ads['AR_2'];%>	
	 <% } %> 
         <!-- More Albums --> 
         <div class="photos-wrap">
            <h3 class="main-story-title">
               <a href="javascript:void(0);">More Albums</a>
               <span></span>
            </h3>
            <div class="big-thumb">
               <a href="<%- moreAlbums['solarResults'][0]['url']%>" class="thumb-img">
                  <img class="fp-lazy" src="<%-constant.PLACEHOLDER_IMG_640x362 %>" data-src="<%-common.firstpost_imageOptimization(moreAlbums['solarResults'][0]['image'], 640, 362, '') %>" alt="<%- moreAlbums['solarResults'][0]['title']%>" title="<%- moreAlbums['solarResults'][0]['title']%>">
                  <div class="photo-icon">
                     <svg width="50.45" height="45.239" viewBox="0 0 50.45 45.239"><defs><filter id="a" x="0" y="0" width="50.45" height="45.239" filterUnits="userSpaceOnUse"><feOffset dy="3" input="SourceAlpha"></feOffset><feGaussianBlur stdDeviation="4.5" result="b"></feGaussianBlur><feFlood flood-opacity="0.161"></feFlood><feComposite operator="in" in2="b"></feComposite><feComposite in="SourceGraphic"></feComposite></filter></defs><g transform="translate(13.5 10.5)"><g style="filter:url(#a);" transform="matrix(1, 0, 0, 1, -13.5, -10.5)"><path style="fill:#000;" d="M23.147,3H2.3A1.3,1.3,0,0,0,1,4.3V19.936a1.3,1.3,0,0,0,1.3,1.3H23.147a1.3,1.3,0,0,0,1.3-1.3V4.3A1.3,1.3,0,0,0,23.147,3ZM16.959,7.56A1.628,1.628,0,1,1,15.33,9.188,1.628,1.628,0,0,1,16.959,7.56ZM4.908,17.33,9.226,7.4l4.91,7.949,4.209-2.091,2.2,4.067Z" transform="translate(12.5 7.5)"></path></g></g></svg>
                  </div>
               </a>
               <div class="title-wrap">
                  <h3 class="main-title">
                     <a href="<%- moreAlbums['solarResults'][0]['url']%>"><%- moreAlbums['solarResults'][0]['title']%></a>
                  </h3>
                  <% var shareArr = []; 
			            shareArr['shareTitle'] = moreAlbums['solarResults'][0]['title'];
	                  	shareArr['shareUrl'] = moreAlbums['solarResults'][0]['url']; 
			       %>
			       <%- include(viewpath+ '/widgets/popup_social_share_widget',{shareArr:shareArr}); %>
               </div>
            </div>
         </div>
         <!--photos-wrap-->
         <div class="photos small-thumb-wrap">
            <div class="small-thumb-row">
            	<% for (const [moreAlbums_key, moreAlbums_val] of Object.entries(moreAlbums['solarResults'].slice(1,4))) { %>
	               <div class="small-thumb">
	                  <a href="<%- moreAlbums_val['url']%>" class="thumbnail">
	                     <img class="fp-lazy" src="<%-constant.PLACEHOLDER_IMG_192x104 %>" data-src="<%-common.firstpost_imageOptimization(moreAlbums_val['image'], 192, 104, '') %>" alt="<%- moreAlbums_val['title']%>" title="<%- moreAlbums_val['title']%>" width="200px" height="108px">
	                     <div class="s-photo-icon">
	                        <svg width="50.45" height="45.239" viewBox="0 0 50.45 45.239"><defs><filter id="a" x="0" y="0" width="50.45" height="45.239" filterUnits="userSpaceOnUse"><feOffset dy="3" input="SourceAlpha"></feOffset><feGaussianBlur stdDeviation="4.5" result="b"></feGaussianBlur><feFlood flood-opacity="0.161"></feFlood><feComposite operator="in" in2="b"></feComposite><feComposite in="SourceGraphic"></feComposite></filter></defs><g transform="translate(13.5 10.5)"><g style="filter:url(#a);" transform="matrix(1, 0, 0, 1, -13.5, -10.5)"><path style="fill:#000;" d="M23.147,3H2.3A1.3,1.3,0,0,0,1,4.3V19.936a1.3,1.3,0,0,0,1.3,1.3H23.147a1.3,1.3,0,0,0,1.3-1.3V4.3A1.3,1.3,0,0,0,23.147,3ZM16.959,7.56A1.628,1.628,0,1,1,15.33,9.188,1.628,1.628,0,0,1,16.959,7.56ZM4.908,17.33,9.226,7.4l4.91,7.949,4.209-2.091,2.2,4.067Z" transform="translate(12.5 7.5)"></path></g></g></svg>
	                     </div>
	                  </a>
	                  <a href="<%- moreAlbums_val['url']%>" class="copy">
	                  	<%= moreAlbums_val['title']%>
	                  </a>
	                  <% var shareArr = []; 
				            shareArr['shareTitle'] = moreAlbums_val['title'];
		                  	shareArr['shareUrl'] = moreAlbums_val['url']; 
				       %>
				       <%- include(viewpath+ '/widgets/popup_social_share_widget',{shareArr:shareArr}); %>
	               </div>
               <% } %>
            </div>
            <a href="<%=constant.SITE_URL%>tag/<%- category_name%>" class="see-all">

<%- include(viewpath+ '/layouts/header'); %>
<div class="container">
   <% if (typeof ads  !== 'undefined' && typeof ads['ATF_728']  !== 'undefined'){%>
        <div class="ad-center">
            <%-common.displayAds(ads['ATF_728'][0]) %>
        </div>
    <% } %>
   <!--cricket-ticker-wrap-->
   <%- include(viewpath+ '/widgets/cricketscore_strip'); %>
   <!--cricket-ticker-wrap-->

   <% if(typeof constant.config_flags.EXIT_POLL_FLAG !== 'undefined' && constant.config_flags.EXIT_POLL_FLAG == '1') { %>
     <!--panchayat-election-widget-->
     <%- include(viewpath+ '/widgets/five_states_result_widget_2021.ejs'); %> 
     <!--panchayat-election-widget-->
   <% } %>
   
   <%- include(viewpath+ '/widgets/breadCrumbs'); %>
   <div class="main-container">
      <div class="main-content">
      	<% if(pgNo == '1' || typeof pgNo =='undefined') { %> 
         <div class="main-story-thumb-wrap">
            <h3 class="main-story-title">
               <a href="javascript:void(0);">Latest Photos</a>
               <span></span>
            </h3>
            <div class="slider-wrap main-story-slider" id="mainstoryslider">
               <div class="glide__track" data-glide-el="track">
                  <ul class="slider">
                  	<% for (const [categoryData_key, categoryData_val] of Object.entries(categoryData['CatMainRanking']['main_ranking'])) { %>
                     <li class="main-story-thumb">
                        <a href="<%- categoryData_val['modified_posturl'].replace(constant.MAIN_SITE_URL, constant.SITE_URL)%>" class="thumb">
                        	<img src="<%- categoryData_val['desktop_img']%>" alt="<%- categoryData_val['modified_title']%>" title="<%- categoryData_val['modified_title']%>">
                        	<div class="photo-icon">
                              <svg width="50.45" height="45.239" viewBox="0 0 50.45 45.239"><defs><filter id="a" x="0" y="0" width="50.45" height="45.239" filterUnits="userSpaceOnUse"><feOffset dy="3" input="SourceAlpha"></feOffset><feGaussianBlur stdDeviation="4.5" result="b"></feGaussianBlur><feFlood flood-opacity="0.161"></feFlood><feComposite operator="in" in2="b"></feComposite><feComposite in="SourceGraphic"></feComposite></filter></defs><g transform="translate(13.5 10.5)"><g style="filter:url(#a);" transform="matrix(1, 0, 0, 1, -13.5, -10.5)"><path style="fill:#000;" d="M23.147,3H2.3A1.3,1.3,0,0,0,1,4.3V19.936a1.3,1.3,0,0,0,1.3,1.3H23.147a1.3,1.3,0,0,0,1.3-1.3V4.3A1.3,1.3,0,0,0,23.147,3ZM16.959,7.56A1.628,1.628,0,1,1,15.33,9.188,1.628,1.628,0,0,1,16.959,7.56ZM4.908,17.33,9.226,7.4l4.91,7.949,4.209-2.091,2.2,4.067Z" transform="translate(12.5 7.5)"></path></g></g></svg>
                           </div>
                        </a>
                        <div class="title-wrap">
                           <a href="<%-categoryData_val['category_url'] %>" class="category-name"><%-categoryData_val['category'] %></a>
                           <h1 class="main-title">
                              <a href="<%-categoryData_val['modified_posturl'].replace(constant.MAIN_SITE_URL, constant.SITE_URL) %>"><%-categoryData_val['modified_title'] %></a>
                           </h1>
                           <% var shareArr = []; 
		                        shareArr['shareTitle'] = categoryData_val['modified_title'];
		                        shareArr['shareUrl'] = categoryData_val['modified_posturl']; 
		                   %>
                           <%- include(viewpath+ '/widgets/popup_social_share_widget',{shareArr:shareArr}); %>
                        </div>
                     </li>
                    <% } %>
                  </ul>
                  <div class="glide__bullets" data-glide-el="controls[nav]">
		     <% i = 0; %>
                     <% for (const [categoryData_key, categoryData_val] of Object.entries(categoryData['CatMainRanking']['main_ranking'])) { %>
		               <button class="glide__bullet" data-glide-dir="=<%=i %>"></button>
			<% i++; %>
		     <% } %>
                  </div>
               </div>
            </div>
         </div>
         <!--main-story-thumb-->
         <% for (const [categoryData_key, categoryData_val] of Object.entries(categoryData['CatMainRanking']['related_ranking'])) { %>
         <div class="big-thumb">
            <a href="<%- categoryData_val['modified_posturl'].replace(constant.MAIN_SITE_URL, constant.SITE_URL)%>" class="thumb-img">
            	<img class="fp-lazy" src="<%-constant.PLACEHOLDER_IMG_640x362 %>" data-src="<%-common.firstpost_imageOptimization(categoryData_val['post_image'], 640, 362, '') %>" alt="<%- categoryData_val['modified_title']%>" title="<%- categoryData_val['modified_title']%>">

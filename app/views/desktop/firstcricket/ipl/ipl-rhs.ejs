                    .temper{text-align: center;font: 700 41px/45px Arial;letter-spacing: 0px;color: #FFFFFF;opacity: 1;}
                    .temvot{text-align: center;font: 400 12px/15px Arial;letter-spacing: 0px;color: #FFFFFF;opacity: 1;}

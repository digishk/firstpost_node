        .matchdate{text-align: left;font: 400 14px/17px Arial;letter-spacing: 0.56px;color: #000000;opacity: 1;margin: 5px 0}
        .matchwin{text-align: left;font: italic 400 12px/17px Arial;letter-spacing: 0.48px;color: #FAD65F;opacity: 1;}
        .recentmat li{padding:15px;border-bottom: 1px solid #ccc}
        .recentmat li:last-child{border-bottom: 0}
        .recentmatwrap{padding: 0 0 5px;border: 1px solid #DEDEDE;position: relative;top: -15px;margin: 0 0 20px;border-bottom: 4px solid #000;}
        .allmat{text-align: center;font: italic  600 16px/20px Arial;letter-spacing: 0px;color: #000000;opacity: 1; display: block;margin-top: 20px}
        
        </style>
        <div class="cricket-score common-widget">
            <div class="double-title">
                <h3 class="title-txt">Recent matches</h3>
                <span class="title-shadow">Recent matches</span>
            </div>
            <div class="recentmatwrap">
               <ul class="recentmat">
                <% let count = 0; %>
                <% var reverseMatchList = Object.entries(resultMatchList).reverse();
                for (const [match_key, match_val] of reverseMatchList) { %>
                <% if (count >= 5) break;%>
                <% if (match_val['category'] == 'men') { %>
                  <li>
                    <a href="<%= constant.SITE_URL %>firstcricket/cricket-live-score/<%-match_val['teama'].replace(/\s+/g, '-').toLowerCase() %>-vs-<%-match_val['teamb'].replace(/\s+/g, '-').toLowerCase() %>-<%= match_val['matchtype'].replace(/\s+/g, '-').toLowerCase() %>-live-cricket-score-full/<%= match_val['series_Id']%>/<%= match_val['match_Id']%>.html" title="<%-match_val['teama'] %> vs <%-match_val['teamb'] %> Full Scorecard">
                        <div class="matchname"><%= match_val['teama_short']%> <span>Vs</span> <%= match_val['teamb_short']%></div>
                        <div class="matchdate"><%= match_val['seriesname']%></div>
                        <div class="matchwin"><%= match_val['matchresult']%></div>
                    </a>
                  </li>
                  <% count++;%>
                <%  } } %>                
                </ul>
                <a href="<%-constant.SITE_URL %>firstcricket/results/" class="allmat">All Recent Matches</a>
            </div>
        </div>

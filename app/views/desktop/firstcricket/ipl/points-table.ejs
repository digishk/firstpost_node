<%- include(viewpath+ '/layouts/header'); %>
        <div class="main-wrapper">

            <!--Cricket Ticker-->
            <%- include(viewpath+ '/widgets/scorecard_caraousal'); %>
            <input type="hidden" name="" id="checkDevice" value="<%=device%>">
            <!--Cricket Ticker-->

            <!--728x90 Ad Space-->
            <% if (typeof ads  !== 'undefined' && typeof ads['ATF_728']  !== 'undefined'){%>
            <div class="ad-slot fixed-ad-banner-fc">
                <%-common.displayAds(ads['ATF_728'][0]) %>
            </div>
            <% } %>
            <!--728x90 Ad Space-->

            <!--Bread crumb-->
            <%- include(viewpath+ '/widgets/breadCrumbs'); %>
            <!--Bread crumb-->

            <!--Main Page Content-->
            <div class="content-wrap" id="photos">
                <div class="left-content">                    
                    <!--Ranking Section-->
                    <section class="ranking-section">
                        <h1 class="page-title"><span><%= seriesName %></span></h1>
                        <%- include(viewpath+ '/widgets/cric-series-menu'); %>

                        <% if(templateVars && templateVars['pointsData'] && templateVars['pointsData']['standings'] && templateVars['pointsData']['standings']['stage1'] && templateVars['pointsData']['standings']['stage1']['team']) { %>                             
                            <div class="big-title-heading">
                                <h2 class="big-heading-text">
                                    <%= seriesName %> Points Table
                                </h2>
                            </div>
                            <table class="general-tbl ipl-points-table">
                                <thead>
                                    <tr>
                                        <th>Pos</th>
                                        <th>Team</th>
                                        <th>Plyd.</th>
                                        <th>Won</th>
                                        <th>Lost</th>
                                        <th>Tied</th>
                                        <th>Net RR.</th>
                                        <th>Pts</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% const pointsData = templateVars['pointsData']['standings']['stage1']['team'] %>          
                                    <% pointsData.forEach((data, index) => { 
                                        const {name,short_name,pts,nrr,p,w,l,t,pos} = data%>
                                        <tr>
                                            <td><%= pos %></td>
                                            <td>
                                                <a href="https://www.firstpost.com/firstcricket/teams/<%= name.replace(/\s/g,'-').toLowerCase() %>-team-players/series/ipl-<%=iplYear%>.html">
                                                    <%= name %> <p>(<%= short_name %>)</p>
                                                </a>
                                            </td>
                                            <td><%= p %></td>
                                            <td><%= w %></td>
                                            <td><%= l%></td>
                                            <td><%= t %></td>
                                            <td><%= nrr %></td>
                                            <td><%= pts %></td>
                                        </tr>                     
                                    <% }) %>
                                </tbody>
                            </table>
                        <%}%>

                        <div class="ipl-summary-wrap">
                            <h2 class="article-title">
                                <%= seriesName %> Points table
                            </h2>

                            <div class="article-desc">
                                <%- iplText %>
                            </div>
                        </div>

                        <h2 class="page-title"><span><%= seriesName %> more stories</span></h2>

                        <%- include(viewpath+ '/desktop/firstcricket/ipl/ipl-news-widget'); %>

                    </section>
                        <!--Misc Story Section-->
                    <!--Ranking Section-->
                    <% if (typeof outbrain_ads  !== 'undefined' && typeof outbrain_ads['AR_2']  !== 'undefined'){%>
                        <%-outbrain_ads['AR_2'];%>
                    <% } %>
                </div>
                <%- include(viewpath+ '/desktop/firstcricket/ipl/ipl-rhs'); %>
            </div>
            <!--Main Page Content-->
        </div>
<%- include(viewpath+ '/layouts/footer'); %>
<%- include(viewpath+ '/desktop/microsites/olympics-header'); %>
<div class="wrapper">
	<div class="cont-wrap" id="home-section">
		<div class="left-section">
			<div class="main-wrap">
				<h1 class="pgttl">
					<span>Big Story</span>
				</h1>
				 <!--Top Slider Section-->
                <section class="top-slider-section">
                    <div class="glide" id="top-slider">
                        <div data-glide-el="track" class="glide__track">
                            <ul class="glide__slides">
                                <% for (const [rankDataKey, rankDataValue] of Object.entries(rankingData['main_ranking'])) { %>
                                <li class="glide__slide">
                                    <div class="top-story-slide">
                                        <div class="top-image-wrap">
                                            <a href="<%=rankDataValue['post_url']%>" title="<%=rankDataValue['post_title']%>">
                                                <img src="<%=rankDataValue['desktop_img']%>" alt="<%=rankDataValue['post_title']%>" title="<%=rankDataValue['post_title']%>"/>
                                            </a>
                                            <div class="main-title-div">
                                                <h2 class="main-title-wrap">
                                                    <a href="<%=rankDataValue['post_url']%>" title="<%=rankDataValue['post_title']%>">
                                                        <%=rankDataValue['post_title']%>
                                                    </a>
                                                </h2>
                                                <div class="share-wrap">
                                                    <% var shareArr = []; 
                                                    shareArr['shareTitle'] = rankDataValue['post_title'];
                                                    shareArr['shareUrl'] = rankDataValue['post_url']; %>
                                                    <%- include(viewpath+ '/widgets/popup_social_share_widget',{shareArr:shareArr}); %>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </li>
                                <% } %>                     
                            </ul>
                        </div>
                        <div class="glide__bullets" data-glide-el="controls[nav]">
                            <button class="glide__bullet" data-glide-dir="=0"></button>
                            <button class="glide__bullet" data-glide-dir="=1"></button>
                            <button class="glide__bullet" data-glide-dir="=2"></button>
                          </div>
                    </div>
                </section>
                <!--Top Slider Section-->
                <!-- Latest News Section -->
                <section class="latestnews-section">
                    <h2 class="pgttl"><span>LATEST NEWS</span></h2>
                    <a href="https://www.firstpost.com/tag/tokyo-olympics-2020" target="_blank" class="read-more">read more</a>
                    <div class="latest">
                        <div class="glide" id="latest">
                        	<div class="glide__track" data-glide-el="track">
                                <ul class="glide__slides">
                                    <% for (const [fact_key, fact_val] of Object.entries(latestNewsData['solarResults'])) { %>
                                    <% if(fact_key <= '2' ){ %>
                                    <li class="glide__slide">
                                        <div class="photo-box">
											<div class="image-wrap">
												<a href="<%= fact_val['url'] %>"><img src="<%= fact_val['image'] %>" alt="<%= fact_val['title'] %>"></a>
											</div>
											<div class="photo-content">
												<h3 class="ph-ttl"><a href="<%= fact_val['url'] %>"><%= fact_val['title'] %></a></h3>
												<div class="share-wrap">
													<% var shareArr = []; 
                                                    shareArr['shareTitle'] = fact_val['title'];
                                                    shareArr['shareUrl'] = fact_val['url']; %>
                                                    <%- include(viewpath+ '/widgets/popup_social_share_widget',{shareArr:shareArr}); %>
												</div>
											</div>
										</div>
                                    </li>
                                    <% } } %>
                                </ul>
                            </div>
                        </div>
                    </div>
                </section>
                <!-- Latest News Section -->
                <!-- Videos Section -->
                <section class="videos-section">
                    <h2 class="pgttl"><span>FirstPost in Tokyo</span></h2>
                    <a href="<%-constant.SITE_URL %>tag/firstpost-in-tokyo" class="read-more">read more</a>
                    <div class="videos-slider">
                        <div class="glide" id="vdoslider">
                            <div class="glide__track" data-glide-el="track">
                                <ul class="glide__slides">
                                    <% for (const [video_key, video_val] of Object.entries(videos_olympics['solarResults'])) { %>
                                    <li class="glide__slide">
                                        <div class="photo-box">
                                            <div class="image-wrap">
                                                <a href="<%= video_val['url'] %>"><img src="<%= video_val['image'] %>" alt="<%= video_val['title'] %>"></a>
                                            </div>
                                            <div class="photo-content">
                                                <h3 class="ph-ttl"><a href="<%= video_val['url'] %>"><%= video_val['title'] %></a></h3>
                                                <div class="share-wrap">
                                                    <% var shareArr = []; 
                                                    shareArr['shareTitle'] = video_val['title'];
                                                    shareArr['shareUrl'] = video_val['url']; %>
                                                    <%- include(viewpath+ '/widgets/popup_social_share_widget',{shareArr:shareArr}); %> 
                                                </div>
                                            </div>
                                        </div>
                                    </li>
                                    <% } %>
                                </ul>
                            </div>
                            <div class="glide__arrows" data-glide-el="controls">
                                <button class="glide__arrow glide__arrow--left" data-glide-dir="<">
                                    <svg xmlns="http://www.w3.org/2000/svg" height="24" viewBox="0 0 24 24" width="24"><path d="M0 0h24v24H0z" fill="none"></path><path d="M20 11H7.83l5.59-5.59L12 4l-8 8 8 8 1.41-1.41L7.83 13H20v-2z"></path></svg>
                                </button>
                                <button class="glide__arrow glide__arrow--right" data-glide-dir=">"><svg xmlns="http://www.w3.org/2000/svg" height="24" viewBox="0 0 24 24" width="24"><path d="M0 0h24v24H0z" fill="none"></path><path d="M12 4l-1.41 1.41L16.17 11H4v2h12.17l-5.58 5.59L12 20l8-8z"></path></svg></button>
                              </div>
                        </div>
                    </div>
                </section>

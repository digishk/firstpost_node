<%- include(viewpath+ '/desktop/microsites/olympics-header'); %>
	<div class="wrapper">
		<div class="breadcrumbs">
			<ul>
				<li><a href="<%=constant.SITE_URL%>sports/olympics/">Olympic 2020</a></li>
				<li><span>Olympic Games Photo</span></li>
			</ul>
		</div>
		<div class="cont-wrap">
			<div class="left-section">
				<div class="photos-wrap">
					<h1 class="pgttl">
						<span>Olympic Games Photo</span>
					</h1>
					<div class="phgallery-wrap">
						<% for (const [photo_key, photo_val] of Object.entries(photos_olympics['solarResults'])) { %>
						<div class="photo-box">
							<div class="image-wrap">
								<div class="icon-photo">
									<svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="50.45" height="45.239" viewBox="0 0 50.45 45.239"><defs><filter id="a" x="0" y="0" width="50.45" height="45.239" filterUnits="userSpaceOnUse"><feOffset dy="3" input="SourceAlpha"></feOffset><feGaussianBlur stdDeviation="4.5" result="b"></feGaussianBlur><feFlood flood-opacity="0.161"></feFlood><feComposite operator="in" in2="b"></feComposite><feComposite in="SourceGraphic"></feComposite></filter></defs><g transform="translate(13.5 10.5)"><g style="filter:url(#a);" transform="matrix(1, 0, 0, 1, -13.5, -10.5)"><path style="fill:#000;" d="M23.147,3H2.3A1.3,1.3,0,0,0,1,4.3V19.936a1.3,1.3,0,0,0,1.3,1.3H23.147a1.3,1.3,0,0,0,1.3-1.3V4.3A1.3,1.3,0,0,0,23.147,3ZM16.959,7.56A1.628,1.628,0,1,1,15.33,9.188,1.628,1.628,0,0,1,16.959,7.56ZM4.908,17.33,9.226,7.4l4.91,7.949,4.209-2.091,2.2,4.067Z" transform="translate(12.5 7.5)"></path></g></g></svg>
								</div>
								<a href="<%= photo_val['url'] %>"><img src="<%= photo_val['image'] %>" alt="<%= photo_val['title'] %>"></a>
							</div>
							<div class="photo-content">
								<h3 class="ph-ttl"><a href="<%= photo_val['url'] %>"><%= photo_val['title'] %></a></h3>
								<div class="share-wrap">
									<% var shareArr = []; 
                                    shareArr['shareTitle'] = photo_val['title'];
                                    shareArr['shareUrl'] = photo_val['url']; %>
                                    <%- include(viewpath+ '/widgets/popup_social_share_widget',{shareArr:shareArr}); %>
								</div>
							</div>
						</div>
						<% } %>
					</div>
					<div class="pagination">
						<a href="" class="prev-btn"><i class="icon"><svg height="24px" viewBox="0 0 24 24" width="24px" fill="#a7a7a7"><path d="M0 0h24v24H0V0z" fill="none"/><path d="M20 11H7.83l5.59-5.59L12 4l-8 8 8 8 1.41-1.41L7.83 13H20v-2z"/></svg></i><span>Previous</span></a>
						<a href="" class="next-btn"><span>Next</span><i class="icon"><svg height="24px" viewBox="0 0 24 24" width="24px" fill="#a7a7a7"><path d="M0 0h24v24H0V0z" fill="none"/><path d="M12 4l-1.41 1.41L16.17 11H4v2h12.17l-5.58 5.59L12 20l8-8-8-8z"/></svg></i></a>
					</div>
				</div>
			</div>
			<%- include(viewpath+ '/desktop/microsites/olympics-rhs'); %>
		</div>
	</div>
<%- include(viewpath+ '/desktop/microsites/olympics-footer'); %>	
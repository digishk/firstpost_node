<%- include(viewpath+ '/desktop/microsites/olympics-header'); %>
	<div class="wrapper">
		<div class="breadcrumbs">
			<ul>
				<li><a href="<%=constant.SITE_URL%>sports/olympics/">Olympic 2020</a></li>
				<li><span>Olympic Videos</span></li>
			</ul>
		</div>
		<div class="cont-wrap">
			<div class="left-section">
				<div class="videos-wrap">
					<h1 class="pgttl">
						<span>Olympic Videos</span>
					</h1>
					<div class="phgallery-wrap">
						<% for (const [video_key, video_val] of Object.entries(videos_olympics['solarResults'])) { %>
						<div class="photo-box">
							<div class="image-wrap">
								<div class="icon-video">
                                    <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="57.887" height="57.887" viewBox="0 0 57.887 57.887"><defs><style>.a{fill:#fff;}.b{filter:url(#a);}</style><filter id="a" x="0" y="0" width="57.887" height="57.887" filterUnits="userSpaceOnUse"><feOffset dy="3" input="SourceAlpha"/><feGaussianBlur stdDeviation="4.5" result="b"/><feFlood flood-opacity="0.294"/><feComposite operator="in" in2="b"/><feComposite in="SourceGraphic"/></filter></defs><g class="b" transform="matrix(1, 0, 0, 1, 0, 0)"><path class="a" d="M17.443,2A15.443,15.443,0,1,0,32.887,17.443,15.449,15.449,0,0,0,17.443,2ZM14.355,24.393v-13.9l9.266,6.95Z" transform="translate(11.5 8.5)"/></g></svg>
                                </div>
								<a href="<%= video_val['url'] %>"><img src="<%= video_val['image'] %>" alt="<%= video_val['title'] %>"></a>
							</div>
							<div class="photo-content">
								<h3 class="ph-ttl"><a href="<%= video_val['url'] %>"><%= video_val['title'] %></a></h3>
								<div class="share-wrap">
									<% var shareArr = []; 
                                    shareArr['shareTitle'] = video_val['title'];
                                    shareArr['shareUrl'] = video_val['url']; %>
                                    <%- include(viewpath+ '/widgets/popup_social_share_widget',{shareArr:shareArr}); %>	
								</div>
							</div>
						</div>
						<% } %>
					</div>
					<div class="pagination">
						<a href="" class="prev-btn"><i class="icon"><svg height="24px" viewBox="0 0 24 24" width="24px" fill="#a7a7a7"><path d="M0 0h24v24H0V0z" fill="none"/><path d="M20 11H7.83l5.59-5.59L12 4l-8 8 8 8 1.41-1.41L7.83 13H20v-2z"/></svg></i><span>Previous</span></a>
						<a href="" class="next-btn"><span>Next</span><i class="icon"><svg height="24px" viewBox="0 0 24 24" width="24px" fill="#a7a7a7"><path d="M0 0h24v24H0V0z" fill="none"/><path d="M12 4l-1.41 1.41L16.17 11H4v2h12.17l-5.58 5.59L12 20l8-8-8-8z"/></svg></i></a>
					</div>
				</div>
			</div>
			<%- include(viewpath+ '/desktop/microsites/olympics-rhs'); %>
		</div>
	</div>
<%- include(viewpath+ '/desktop/microsites/olympics-footer'); %>
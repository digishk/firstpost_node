                <th width="10%"><img src="<%-constant.SITE_URL %>static/images/olympics/bronze.png" alt="" /></th>
                <th width="20%">total</th>
            </tr>
            <% if(templateVars && templateVars['medalData'] && templateVars['medalData']['medals_tally'] ) { %>
            <% const medalLeaderboard = templateVars['medalData']['medals_tally'] %>      
            <% medalLeaderboard.forEach((data, index) => { 
                const {rank,team_name,short_team_name,gold_count,silver_count,bronze_count,medals_count} = data
                const serialNo = index+1
                if(serialNo > 10) return %>
            <tr class="<% if(team_name == 'India'){%>active<%}%>">
                <td  width="3%"><%- rank %></td>
                <td  width="48%">
                    <span><%- short_team_name %></span>
                </td>
                <td width="10%"><%- gold_count %></td>
                <td width="10%"><%- silver_count %></td>
                <td width="10%"><%- bronze_count %></td>
                <td width="20%"><%- medals_count %></td>
            </tr>
            <% }) %>
            <% } %>
            <% if(templateVars && templateVars['medalData'] && templateVars['medalData']['medals_tally'] ) { %>
            <% const medalLeaderboard = templateVars['medalData']['medals_tally'] %>

	<div class="daily-quiz-wrap">
		<h3 class="ttl">Daily Quiz</h3>
		<div class="daily-cont">
            <div class="daily-container">
                <% for (const [glide_key, glide_val] of Object.entries(transformData)) { %>
                    <% var no = parseInt(glide_key)+1; %>
                <div class="quesItem active">
                    <div class="errOption" style="display: none;">Please select an option</div>
                    <h3 class="ques"><%= no %> <%= glide_val['question'].replace(/[^a-zA-Z0-9'"â€™.?, ]/g,"") %></h3>
                    <div class="options">
                        <ol>
                            <% for (const [option_key, option_val] of Object.entries(glide_val['options'])) { %>
                                <% var no = parseInt(option_key)+1; var radioId= 'radio'+no; var correctAnswer = parseInt(glide_val['correctOption']); %>
                                    <li>
                                        <input type="radio" id="<%=radioId %>" name="ques1" 
                                        value="<%= no == correctAnswer ? "1" : "0" %>">
                                        <label for="<%=radioId %>"><span><span></span></span><em>
                                            <%= option_val %></em></label></li>
                            <% } %>

                            <!-- <li><input type="radio" id="radio2" name="ques1" value="<%= 2 == quizData['correct_option'][glide_key] ? "1" : "0" %>"><label for="radio2"><span><span></span></span><em><%= quizData['option_2'][glide_key] %></em></label></li>
                            <li><input type="radio" id="radio3" name="ques1" value="<%= 3 == quizData['correct_option'][glide_key] ? "1" : "0" %>"><label for="radio3"><span><span></span></span><em><%= quizData['option_3'][glide_key] %></em></label> </li>
                            <li><input type="radio" id="radio4" name="ques1" value="<%= 4 == quizData['correct_option'][glide_key] ? "1" : "0" %>"><label for="radio4"><span><span></span></span><em><%= quizData['option_4'][glide_key] %></em></label></li>
                         -->
                        </ol>
                    </div>
                </div>
                <% } %>
            </div>

            <div class="quizAnswer"  style="display: none;">
            <h3 class="ques">Thank you for taking the quiz</h3>
            <h4 class="ques">You have attempted <%= quizData['noOfQues'] %> questions </h4>
            <h3 class="answer" id="answer"><span><span></span></span><em>Your Total Score is: 100</em></h3> 
            </div>

			<div class="pagination" id="quiztest">
				<div class="quesnos"><span id="qsno">1</span> of <%= quizData['noOfQues'] %></div>
				<a href="javascript:void(0);" class="next-btn" onclick="next()"><span>Next</span><i class="icon"><svg height="24px" viewBox="0 0 24 24" width="24px" fill="#a7a7a7"><path d="M0 0h24v24H0V0z" fill="none"></path><path d="M12 4l-1.41 1.41L16.17 11H4v2h12.17l-5.58 5.59L12 20l8-8-8-8z"></path></svg></i></a>
                <a href="javascript:void(0);" class="submit-btn"  onclick="submitQuiz()" style="display:none"><span>Submit</span></a>
			</div>
		</div>

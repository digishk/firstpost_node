<%- include(viewpath+ '/layouts/m-header'); %>
        <div class="main-wrapper">
            <!--Ad Slot-->
            <% if (typeof ads  !== 'undefined' && typeof ads['ATF_320']  !== 'undefined'){%>
                <div class="ad-center">
                    <%-common.displayAds(ads['ATF_320'][0]) %>
                </div>
            <% } %>
            <!--Ad Slot-->

            <!--Cricket Ticker-->
            <%- include(viewpath+ '/widgets/scorecard_caraousal'); %>
            <!--Cricket Ticker-->

            <!--Bread crumb-->
            <%- include(viewpath+ '/widgets/breadCrumbs'); %>
            <!--Bread crumb-->

            <!--Cricket Inner Menu-->
            <%- include(viewpath+ '/layouts/level-4-nav'); %>            
            <!--Cricket Inner Menu-->

            <!--Main Page Content-->
            <div class="main-content" id="ranking">
                <!--Ranking Section-->
                <section class="ranking-section">
                    <h1 class="page-title"><span>ICC TEAMS RANKINGS</span></h1>
                    <% for (const [rank_key, rank_val] of Object.entries(rankData)) { %>
                            <div class="big-title-heading">
                                <h2 class="big-heading-text">
                                    <%=rank_key%> Rankings
                                </h2>
                            </div>
                            <table class="general-tbl">
                                <thead>
                                    <tr>
                                        <th>Rank</th>
                                        <th>Team</th>
                                        <th>Matches</th>
                                        <th>Points</th>
                                        <th>Rating</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <%                                 
                                    for (const [r_key, r_val] of Object.entries(rank_val['bat-rank']['rank'])) { %>
                                    <tr>
                                        <td><%=r_val['no']%></td>
                                        <td><%=r_val['Country']%></td>
                                        <td><%=r_val['Matches']%></td>
                                        <td><%=r_val['Points']%></td>
                                        <td><%=r_val['Rating']%></td>
                                    </tr>
                                    <%}%>
                                </tbody>
                            </table>
                            <!--Ad Slot-->
                            <% if ((typeof ads  !== 'undefined' && typeof ads['BTF_300'] !== 'undefined')){%>
                                <div class="ad-center BTF_300" data-ad-id="<%-ads['BTF_300'][0]%>" data-ad-size="<%-ads['BTF_300'][1]%>">
                                    <%-common.displayAds(ads['BTF_300'][0]) %>
                                </div>
                            <% } %>
                            <!--Ad Slot-->
                    <%}%>
                </section>
                <!--Ranking Section-->
            </div>
            <!--Main Page Content-->
            <%- include(viewpath+ '/widgets/mailChimp'); %>
            <% if (typeof outbrain_ads  !== 'undefined' && typeof outbrain_ads['MB_2']  !== 'undefined'){%>
                <%-outbrain_ads['MB_2'];%>
            <% } %>
        </div>

<%- include(viewpath+ '/layouts/m-footer'); %>
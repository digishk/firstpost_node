   <% if((pgNo == '1' || typeof pgNo =='undefined') && subCat !== null) { %> 
   <div class="main-story-thumb-wrap">
      <h3 class="main-story-title">
         <a href="javascript:void(0);">THE BIG story</a>
         <span></span>
      </h3>
      <div class="slider-wrap main-story-slider" id="mainstoryslider">
         <div class="glide__track" data-glide-el="track">
            <ul class="slider">
               <li class="main-story-thumb">
                  <!--<div class="live-txt"><span class="blinking"></span>live</div>-->
                 <a href="<%=subCat['showsha'][sub_category_name]['post_url'].replace(constant.MAIN_SITE_URL, constant.SITE_URL) %>" class="thumb">
                  <% if (typeof subCat['showsha'][sub_category_name]['post_image_mobile'] !== 'undefined' && subCat['showsha'][sub_category_name]['post_image_mobile'] != '') { %>
                     <img src="<%- subCat['showsha'][sub_category_name]['post_image_mobile'] %>" alt="<%=subCat['showsha'][sub_category_name]['post_title'] %>" title="<%=subCat['showsha'][sub_category_name]['post_title'] %>">
                  <% } else { %>
                     <img src="<%- subCat['showsha'][sub_category_name]['post_image'] %>" alt="<%=subCat['showsha'][sub_category_name]['post_title'] %>" title="<%=subCat['showsha'][sub_category_name]['post_title'] %>">
                  <% } %>
                  </a>
                  <div class="title-wrap">
                     <h1 class="main-title">
                        <a href="<%=subCat['showsha'][sub_category_name]['post_url'].replace(constant.MAIN_SITE_URL, constant.SITE_URL) %>"><%=subCat['showsha'][sub_category_name]['post_title'] %></a>
                     </h1>
                     <% var shareArr = []; 
                        shareArr['shareTitle'] = subCat['showsha'][sub_category_name]['post_title'];
                        shareArr['shareUrl'] = subCat['showsha'][sub_category_name]['post_url']; 
                     %>
                       <%- include(viewpath+ '/widgets/popup_social_share_widget',{shareArr:shareArr}); %>
                  </div>
               </li>
            </ul>
         </div>
      </div>
   </div>
   <!--main-story-thumb-->
   <% } %>
      <% var i = 0; 
      for (const [subCatData_key, subCatData_val] of Object.entries(subCatData['solarResults'])) {
         i++;
      %>
         <div class="big-thumb">
            <a href="<%- subCatData_val['url'].replace(constant.MAIN_SITE_URL, constant.SITE_URL)%>" class="thumb-img">
            <img class="fp-lazy" src="<%-constant.PLACEHOLDER_IMG_320x180 %>" data-src="<%- common.firstpost_imageOptimization(subCatData_val['image'], 320, 180, '') %>" alt="<%- subCatData_val['title']%>" title="<%- subCatData_val['title']%>">
            </a>
            <div class="title-wrap">
               <a href="<%=constant.SITE_URL%>category/<%- subCatData_val['category'].replace(/\s+/g, '-').toLowerCase()%>" class="category-name"><%- subCatData_val['category']%></a>
               <h3 class="main-title">
                  <a href="<%- subCatData_val['url'].replace(constant.MAIN_SITE_URL, constant.SITE_URL)%>"><%= subCatData_val['title']%></a>
               </h3>
               <p class="copy">
                  <%= subCatData_val['description']%>
               </p>
               <% var shareArr = []; 
                  shareArr['shareTitle'] = subCatData_val['title'];
                  shareArr['shareUrl'] = subCatData_val['url']; 
               %>
               <%- include(viewpath+ '/widgets/popup_social_share_widget',{shareArr:shareArr}); %>
            </div>
         </div>
         <% if(i == 5) {%>
            <% if (typeof ads  !== 'undefined' && typeof ads['ATF_300']  !== 'undefined'){%>
               <div class="ad-center">
                  <%-common.displayAds(ads['ATF_300'][0]) %>
               </div>
            <% } %>
         <% } %>
   <% } %>

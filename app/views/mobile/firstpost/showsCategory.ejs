<%- include(viewpath+ '/layouts/m-header'); %>

   <!--cricket-ticker-wrap-->
   <%- include(viewpath+ '/widgets/m-cricketscore_strip'); %>
   <!--cricket-ticker-wrap-->

         <% if (typeof ads  !== 'undefined' && typeof ads['ATF_320']  !== 'undefined'){%>
          <div class="ad-center">
              <%-common.displayAds(ads['ATF_320'][0]) %>
            </div>
          <% } %>
         <%- include(viewpath+ '/widgets/breadCrumbs'); %>

         <div class="show-ticker-wrap">
            <div class="slider-wrap cricket-ticker-slider showtickerslider" id="showtickerslider">
               <div class="glide__track" data-glide-el="track">
                  <div class="slider">
                     <% for (const [showList_key, showList_val] of Object.entries(showListing)) { %>
                        <a href="<%=constant.SITE_URL%>shows/<%-showList_val['slug'].replace(constant.MAIN_SITE_URL, constant.SITE_URL) %>" class=""><img src="<%-common.firstpost_imageOptimization(showList_val['img_logo_url']) %>"></a>
                     <% } %>
                  </div>
               </div>
               <div class="glide__arrows" data-glide-el="controls">
                        <div class="glide__arrow glide__arrow--prev" data-glide-dir="<"><svg height="24" viewBox="0 0 24 24" width="24"><path d="M15.41 16.59L10.83 12l4.58-4.59L14 6l-6 6 6 6 1.41-1.41z"/><path d="M0 0h24v24H0V0z" fill="none"/></svg></div>
                        <div class="glide__arrow glide__arrow--next" data-glide-dir=">"><svg height="24" viewBox="0 0 24 24" width="24"><path d="M8.59 16.59L13.17 12 8.59 7.41 10 6l6 6-6 6-1.41-1.41z"/><path d="M0 0h24v24H0V0z" fill="none"/></svg></div>
                    </div>
            </div>
         </div><!--cricket-ticker-wrap-->

         <div class="article-content clearfix">
            <%# include(viewpath+ '/widgets/floating_social_share_widget'); %>

            <div class="article-sect">
               <div class="main-story-thumb-wrap">
                  <h3 class="main-story-title shows-title">
                    <% for (const [showList_key, showList_val] of Object.entries(showListing)) { %>
                      <% if(showList_val['slug'] == category_slug) { %> 
                        <img src="<%-common.firstpost_imageOptimization(showList_val['img_logo_url']) %>">
                      <% } %>
                    <% } %>
                  </h3>
                  <h1 class="inner-main-title">
                     <%- postPageJson['post']['post_title']%>
                  </h1>
                  <%- include(viewpath+ '/widgets/social_share_widget'); %>
                  <p class="inner-copy">
                     <%- postPageJson['post']['post_excerpt']%>
                  </p>
                  <%  if(typeof videoURLArr !== 'undefined' && videoURLArr != ''){ %> 
                  <div class="video-con-thumb">
                     <div class="thumb">
                        <%- include(viewpath+ '/widgets/video_player_widget',{videoURLArr:videoURLArr}); %>
                     </div>
                  </div>
                  <% } %> 
               </div>
               <!--main-story-thumb-->
               <!--Episodes-->
               <h3 class="main-story-title">
                  <a href="javascript:void(0);">Episodes</a>
                  <span></span>
               </h3>
               <!--Episode List-->
               <% for (const [episodeArr_key, episodeArr_val] of Object.entries(listingArr)) { %>
                  <% if (episodeArr_val['post']['post_name'] != postPageJson['post']['post_name']) { %>
                     <div class="big-thumb">
                        <a href="<%=constant.SITE_URL%>shows/<%- category_slug %>/<%- episodeArr_val['post']['post_name'] %>-<%- episodeArr_val['post']['ID'] %>.html" class="thumb-img">
                        <img class="fp-lazy" src="<%-constant.PLACEHOLDER_IMG_320x180 %>" data-src="<%-common.firstpost_imageOptimization(episodeArr_val['post']['post_image'], 320, 180, '') %>"  alt="<%- episodeArr_val['post']['post_title']%>" title="<%- episodeArr_val['post']['post_title']%>">
                        <div class="video-icon">
                            <svg width="70.182" height="70.182" viewBox="0 0 70.182 70.182">
                               <defs>
                                  <filter id="a" x="0" y="0" width="70.182" height="70.182" filterUnits="userSpaceOnUse">
                                     <feOffset dy="3" input="SourceAlpha"></feOffset>
                                     <feGaussianBlur stdDeviation="4.5" result="b"></feGaussianBlur>
                                     <feFlood flood-opacity="0.294"></feFlood>
                                     <feComposite operator="in" in2="b"></feComposite>
                                     <feComposite in="SourceGraphic"></feComposite>
                                  </filter>
                               </defs>
                               <g transform="matrix(1, 0, 0, 1, 0, 0)">
                                  <path fill="#fff" d="M23.591,2A21.591,21.591,0,1,0,45.182,23.591,21.6,21.6,0,0,0,23.591,2ZM19.273,33.307V13.875l12.955,9.716Z" transform="translate(11.5 8.5)"></path>
                               </g>
                            </svg>
                         </div>
                        </a>
                        <div class="title-wrap">
                           <h3 class="main-title">
                              <a href="<%=constant.SITE_URL%>shows/<%- category_slug %>/<%- episodeArr_val['post']['post_name'] %>-<%- episodeArr_val['post']['ID'] %>.html"><%- episodeArr_val['post']['post_title']%></a>
                           </h3>
                           <p class="copy">
                              <%- episodeArr_val['post']['post_excerpt']%> 
                           </p>
                           <% var shareArr = []; 
                              shareArr['shareTitle'] = episodeArr_val['post']['post_title'];
                                 shareArr['shareUrl'] = episodeArr_val['post']['post_url']; 
                           %>
                           <%- include(viewpath+ '/widgets/popup_social_share_widget',{shareArr:shareArr}); %>
                        </div>
                     </div>
                  <% } %>
               <% } %>    
            </div>
         </div>

        
          <%- include(viewpath+ '/widgets/paginationDefault'); %>  
          <% if (typeof ads  !== 'undefined' && typeof ads['ATF_300']  !== 'undefined'){%>
            <div class="ad-center">
                <%-common.displayAds(ads['ATF_300'][0]) %>
            </div>
          <% } %>
      </div>
      <% if (typeof outbrain_ads  !== 'undefined' && typeof outbrain_ads['MB_2']  !== 'undefined'){%>
              <%-outbrain_ads['MB_2'];%>	
      <% } %>
      <%- include(viewpath+ '/widgets/mailChimp'); %>
      <%- include(viewpath+ '/widgets/most_read_widget'); %>

      <!-- Apester code -->
        <!--<div class="apester-media" data-token="57026ab0d51383a359ae688c" data-context="true" data-tags="" data-fallback="true" height="350"></div><script async src="https://static.apester.com/js/sdk/latest/apester-sdk.js"></script>-->
      <!-- Apester code end-->
      <% if (typeof ads  !== 'undefined' && typeof ads['BTF_300']  !== 'undefined'){%>
            <div class="ad-center">
          <%-common.displayAds(ads['BTF_300'][0]) %>
            </div>
          <% } %>
      <%- include(viewpath+ '/widgets/related_article_widget'); %>

      <%- include(viewpath+ '/layouts/m-footer'); %>
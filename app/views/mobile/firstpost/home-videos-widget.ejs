<div class="videos small-thumb-wrap">
	<h3 class="main-story-title">
		<a href="/category/videos" title="Videos">Videos</a>
		<span></span>
	</h3>
	<div class="small-thumb-row inner-fp-lazy">
		<% for (const [homePageJson_key, homePageJson_val] of Object.entries(homePageJson['HPLatestVideos'])) { %>
		<div class="small-thumb">
			<a href="<%-homePageJson_val['VidPostURL'].replace(constant.MAIN_SITE_URL, constant.SITE_URL) %>" class="thumbnail" title="<%-homePageJson_val['PostTitle'] %>">
				<img class="fp-lazy" src="<%-constant.PLACEHOLDER_IMG_192x104 %>" data-src="<%-common.firstpost_imageOptimization(homePageJson_val['PostImage'], 192, 104, '') %>" alt="<%-homePageJson_val['PostTitle'] %>">
				<svg width="70.182" height="70.182" viewBox="0 0 70.182 70.182" class="video-icon"><defs><filter id="a" x="0" y="0" width="70.182" height="70.182" filterUnits="userSpaceOnUse"><feOffset dy="3" input="SourceAlpha"></feOffset><feGaussianBlur stdDeviation="4.5" result="b"></feGaussianBlur><feFlood flood-opacity="0.294"></feFlood><feComposite operator="in" in2="b"></feComposite><feComposite in="SourceGraphic"></feComposite></filter></defs><g transform="matrix(1, 0, 0, 1, 0, 0)"><path fill="#fff" d="M23.591,2A21.591,21.591,0,1,0,45.182,23.591,21.6,21.6,0,0,0,23.591,2ZM19.273,33.307V13.875l12.955,9.716Z" transform="translate(11.5 8.5)"></path></g></svg>
			</a>
			<a href="<%-homePageJson_val['VidPostURL'].replace(constant.MAIN_SITE_URL, constant.SITE_URL) %>" title="<%-homePageJson_val['PostTitle'] %>" class="copy">
				<%-homePageJson_val['PostTitle'] %>
			</a>
			<% var shareArr = []; 
             shareArr['shareTitle'] = homePageJson_val['PostTitle'];
             shareArr['shareUrl'] = homePageJson_val['VidPostURL']; %>
            <%- include(viewpath+ '/widgets/popup_social_share_widget',{shareArr:shareArr}); %>
		</div>
		<% } %>
		<a href="/category/videos" class="see-all" title="See All">see all <svg viewBox="0 0 24 24" ><path d="M8.59 16.59L13.17 12 8.59 7.41 10 6l6 6-6 6-1.41-1.41z"/><path d="M0 0h24v24H0V0z" fill="none"/></svg></a>
	</div>
</div><!--videos-->
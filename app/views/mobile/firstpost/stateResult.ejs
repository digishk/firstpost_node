olor:#abfcc1;text-transform:capitalize;margin:10px 0;font-family:"Alegreya Sans",sans-serif}.trending-hero .copy{font-size:21px;color:#fff;text-transform:capitalize;margin:10px 0 0 0;font-family:"Alegreya Sans",sans-serif;font-weight:300}.trending .thumb-one .copy{color:#fff!important}.trending .thumb-one .tag{color:#abfcc1;font-size:14px;text-transform:capitalize;font-weight:300;font-family:"Alegreya Sans",sans-serif}

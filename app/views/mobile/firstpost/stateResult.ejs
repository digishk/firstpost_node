   <!--cricket-ticker-wrap-->
   <% if (typeof ads  !== 'undefined' && typeof ads['ATF_320']  !== 'undefined'){%>
      <div class="ad-center">
        <%-common.displayAds(ads['ATF_320'][0]) %>
      </div>
    <% } %> 
   <%-/* include(viewpath+ '/widgets/breadCrumbs'); */%>

  <div class="main-story-thumb-wrap">
      <h1 class="main-story-title">
        <a href="javascript:void(0);"><%- common.capitalizeEWLetter(board.replace('-',' ')) %>  Exam Results 2020</a>
        <span></span>
      </h1>
      <div class="board_results_iframe">
          <%-stateBoardIframeUrl%>
      </div>
      <div class="health-title">
        <h2 class="title">Board Results</h2>
      </div>
      <div class="board-flex-wrap">
        <% for (const [list_key, list_val] of Object.entries(boardData)) { %>
        <div class="board_default">
            <h3><%- list_val['name']['english']%></h3>
            <a href="<%=constant.SITE_URL%>board-results/<%- list_val['name']['english'].replace(/\s+/g,'-').toLowerCase() %>-exam-result" class="board-flex align-items cust_link">
                <figure><img src="<%- list_val['logo']%>" alt="<%- list_val['name']['english']%>"></figure>
                <p class="duration title"><%- list_val['message']['english']%></p>
            </a>
            <a href="<%=constant.SITE_URL%>board-results/<%- list_val['name']['english'].replace(/\s+/g,'-').toLowerCase() %>-exam-result" class="view-details">View Details</a>
        </div>
        <% } %>
      </div>

      <section class="news-section">
        <h2 class="page-title"><span>Latest News</span></h2>
      <% if(tag_name != '') { %>
      <% for (const [tagData_key, tagData_val] of Object.entries(tagData['solarResults'])) { %>
          <div class="big-thumb">
            <a href="<%- tagData_val['url']%>" class="thumb-img">
            <img src="<%-constant.PLACEHOLDER_IMG_320x180 %>" class="fp-lazy" data-src="<%-common.firstpost_imageOptimization(tagData_val['image'], 320, 180, '') %>" alt="<%- tagData_val['title']%>" title="<%- tagData_val['title']%>">
            </a>
            <div class="title-wrap">
               <a href="<%=constant.SITE_URL%>category/<%- tagData_val['category'].replace(/\s+/g, '-').toLowerCase()%>" class="category-name"><%- tagData_val['category']%></a>
               <h3 class="main-title">
                  <a href="<%- tagData_val['url']%>"><%= tagData_val['title']%></a>
               </h3>
               <p class="copy">
                  <%= tagData_val['description']%>
               </p>
               <% var shareArr = []; 
                  shareArr['shareTitle'] = tagData_val['title'];
                  shareArr['shareUrl'] = tagData_val['url']; 
               %>
               <%- include(viewpath+ '/widgets/popup_social_share_widget',{shareArr:shareArr}); %>
            </div>
          </div>
      <% } %>
      <% } %>
      </section>
   <!--main-story-thumb-->
   <!--big-thumb-->
    <%- include(viewpath+ '/widgets/paginationDefault'); %>
    <% if (typeof ads  !== 'undefined' && typeof ads['ATF_300']  !== 'undefined'){%>

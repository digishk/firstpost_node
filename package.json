    "staging_restart": "pm2 restart all",
    "staging_stop": "pm2 stop app.js",
    "production_start": "NODE_ENV=env_production pm2 start -i max app.js && pm2 save",
    "production_restart": "pm2 restart all",
    "production_stop": "pm2 stop app.js"
